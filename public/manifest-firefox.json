{
  "name": "Словник української мови (СУМ-11)",
  "description": "Словник української мови (СУМ-11). Використовується сайт sum.in.ua.",
  "version": "0.4.0",
  "manifest_version": 3,
  "action": {
      "default_popup": "index.html",
      "default_title": "Словник української мови"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://sum.in.ua/*",
    "https://sum.in.ua/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "addon_sum@efremov.dev",
      "strict_min_version": "109.0"
    }
  }
}
